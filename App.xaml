<Application x:Class="UMLGenerator.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UMLGenerator"
             xmlns:convert="clr-namespace:WPFLibrary.ValueConverters;assembly=WPFLibrary"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
         <!--#region Converters-->
        <convert:EqualConverter x:Key="EqualConverter"/>
        <convert:BoolToVisibilityCollapsedConverter x:Key="BoolToVisibilityCollapsedConverter"/>
        <!--#endregion-->
    </Application.Resources>
</Application>
